{
    "_id": "biobb_chemistry",
    "desc" : "Biobb_chemistry is the Biobb module collection to perform chemistry over molecular dynamics simulations.", 
    "github" : "https://github.com/bioexcel/biobb_chemistry", 
    "readthedocs" : "https://biobb-chemistry.readthedocs.io/en/latest/", 
    "conda" : "https://anaconda.org/bioconda/biobb_chemistry", 
    "docker" : "https://hub.docker.com/r/mmbirb/biobb_chemistry", 
    "singularity" : "https://www.singularity-hub.org/collections/2886/usage", 
    "version" : "2.0.0",
    "tools" : [
        {
            "block" : "AcpypeParamsAC", 
            "tool" : "ACPype", 
            "desc" : "Small molecule parameterization for AMBER MD package.",
            "exec" : "acpype_params_ac"
        }, 
        {
            "block" : "AcpypeParamsCNS", 
            "tool" : "ACPype", 
            "desc" : "Small molecule parameterization for CNS/XPLOR MD package.",
            "exec" : "acpype_params_cns"
        },
        {
            "block" : "AcpypeParamsGMX", 
            "tool" : "ACPype", 
            "desc" : "Small molecule parameterization for GROMACS MD package.",
            "exec" : "acpype_params_gmx"
        }, 
        {
            "block" : "AcpypeParamsGMXOPLS", 
            "tool" : "ACPype", 
            "desc" : "Small molecule parameterization for OPLS/AA MD package.",
            "exec" : "acpype_params_gmx_opls"
        }, 
        {
            "block" : "BabelConvert", 
            "tool" : "OpenBabel", 
            "desc" : "Small molecule format conversion.",
            "exec" : "babel_convert"
        }, 
        {
            "block" : "BabelAddHydrogens", 
            "tool" : "OpenBabel", 
            "desc" : "Adds hydrogen atoms to small molecules.",
            "exec" : "babel_add_hydrogens"
        }, 
        {
            "block" : "BabelRemoveHydrogens", 
            "tool" : "OpenBabel", 
            "desc" : "Removes hydrogen atoms to small molecules.",
            "exec" : "babel_remove_hydrogens"
        },
        {
            "block" : "BabelMinimize", 
            "tool" : "OpenBabel", 
            "desc" : "Energetically minimize small molecules.",
            "exec" : "babel_minimize"
        }, 
        {
            "block" : "ReduceAddHydrogens", 
            "tool" : "AmberTools Reduce", 
            "desc" : "Adds hydrogen atoms to small molecules.",
            "exec" : "reduce_add_hydrogens"
        },
        {
            "block" : "ReduceRemoveHydrogens", 
            "tool" : "AmberTools Reduce", 
            "desc" : "Removes hydrogen atoms to small molecules.",
            "exec" : "reduce_remove_hydrogens"
        }
    ],
    "dep_pypi" : [
        "install_requires=['biobb_common==2.0.1']", 
        "python_requires='==3.6.*'"
    ], 
    "dep_conda" : [
        "python ==3.6.*", 
        "biobb_common ==2.0.1", 
        "openbabel ==2.4.1"
    ],
    "dep_conda_extra" : [
        "conda install -y -c ambermd ambertools==18.0",
        "conda install -y -c acpype acpype==2018.4.24.22.34.57"
    ], 
    "keywords" : [
        "chemistry", 
        "ambertools", 
        "acpype", 
        "babel"
    ]
}